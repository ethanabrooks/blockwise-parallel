{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=14.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "rescript": "rescript build -with-deps",
    "rescript-w": "rescript build -with-deps -w",
    "tailwind": "tailwindcss -i ./frontend/input.css -o ./dist/output.css",
    "tailwind-w": "tailwindcss -w -i ./frontend/input.css -o ./dist/output.css",
    "parcel": "parcel build ./frontend/index.html",
    "parcel-w": "parcel ./frontend/index.html",
    "gen-form-db": "prisma generate --schema ./databases/form/schema.prisma",
    "gen-inferences-db": "prisma generate --schema ./databases/inferences/schema.prisma",
    "tsc": "tsc",
    "tsc-w": "tsc -w",
    "start": "node dist/server.js",
    "start-w": "nodemon dist/server.js",
    "clean": "rm -rf lib dist databases/form/client databases/inferences/client",
    "build": "yarn rescript && yarn tailwind && yarn parcel && yarn gen-form-db && yarn gen-inferences-db && yarn tsc",
    "push-form-db": "prisma db push --schema ./databases/form/schema.prisma",
    "studio": "prisma studio --schema ./databases/form/schema.prisma"
  },
  "author": "Ethan Brooks",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^5.0.4",
    "@prisma/client": "5.1.1",
    "@rescript/core": "*",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rescript": "^10.1.4",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@glennsl/rescript-fetch": "^0.2.0",
    "@rescript/react": "^0.11.0",
    "@types/node": "^20.5.0",
    "bs-fetch": "^0.6.2",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.3",
    "prettier": "^3.0.0",
    "prisma": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
